@import '/assets/styles/common.wxss';

.tit {
    width: 100%;
    padding: 15px;
}
.tit-ico {
    width: 16px;
    height: 14px;
}
.tit-txt {
    margin-left: 3px;
    font-size: 16px;
    font-weight: bold;
}
.tit-desc {
    margin-left: 5px;
    font-size: 12px;
    color: #484848;
}
.tit-close {
    width: 15px;
    height: 15px;
}
.tit-close image {
    width: 100%;
    height: 100%;
}

.fill {
    height: 44px;
}